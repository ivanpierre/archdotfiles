<Beans>
    <Bean Class="net.sourceforge.squirrel_sql.plugins.dbcopy.prefs.DBCopyPreferenceBean">
        <appendRecordsToExisting>false</appendRecordsToExisting>
        <autoCommitEnabled>true</autoCommitEnabled>
        <checkKeywords>true</checkKeywords>
        <clientName>SQuirreL SQL Client/DBCopy Plugin</clientName>
        <clientVersion>3.9.1/1.15</clientVersion>
        <commitAfterTableDefs>true</commitAfterTableDefs>
        <commitCount>100</commitCount>
        <copyData>true</copyData>
        <copyForeignKeys>true</copyForeignKeys>
        <copyIndexDefs>true</copyIndexDefs>
        <copyPrimaryKeys>false</copyPrimaryKeys>
        <delayBetweenObjects>false</delayBetweenObjects>
        <fileCacheBufferSize>8192</fileCacheBufferSize>
        <promptForDialect>false</promptForDialect>
        <pruneDuplicateIndexDefs>true</pruneDuplicateIndexDefs>
        <recordDelayMillis>0</recordDelayMillis>
        <selectFetchSize>1000</selectFetchSize>
        <tableDelayMillis>0</tableDelayMillis>
        <testColumnNames>true</testColumnNames>
        <useFileCaching>true</useFileCaching>
        <useTruncate>true</useTruncate>
        <writeScript>false</writeScript>
    </Bean>
</Beans>
